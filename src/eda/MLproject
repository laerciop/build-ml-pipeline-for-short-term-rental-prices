name: eda
conda_env: conda.yml

entry_points:
  main:
    parameters:

      nobrowser:
        description: Option to start Jupyter Notebook with the --nobrowser option
        type: string
        default: --no-browser
      port:
        description: Port to expose JN server
        type: float
        default: 5000
    command: jupyter notebook {nobrowser} --port={port}
